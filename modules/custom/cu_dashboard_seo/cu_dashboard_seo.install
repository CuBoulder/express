<?php

function cu_dashboard_seo_install() {
  // Set cu_seo_checklist_google_analytics variable
  if (variable_get('ga_account', NULL)) {
    variable_set('cu_seo_checklist_google_analytics', 1);
  }
}

/**
 * CU Dashboard SEO secure permissions update
 *
 * Rebuild perms for google analytic reports.
 */
function cu_dashboard_seo_update_7001() {
  secure_permissions_rebuild();
}
